{
  "name": "sci-log-db",
  "version": "1.0.0",
  "description": "SciLogDB",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t sci-log-db .",
    "docker:run": "docker run -p 3000:3000 -d sci-log-db",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "start": "node -r source-map-support/register .",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "logs": "pm2 logs",
    "delete-pm2": "pm2 delete 0",
    "start-local": "npm run build && node .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\""
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.0.7",
    "@loopback/authentication-jwt": "^0.7.7",
    "@loopback/authentication-passport": "^5.0.0",
    "@loopback/authorization": "^0.7.7",
    "@loopback/boot": "^3.2.1",
    "@loopback/core": "^2.14.1",
    "@loopback/repository": "^3.4.1",
    "@loopback/rest": "^9.1.3",
    "@loopback/rest-explorer": "^3.1.0",
    "@loopback/service-proxy": "^3.0.7",
    "@types/multer": "^1.4.4",
    "express-session": "^1.17.3",
    "html-to-latex": "^0.8.0",
    "isemail": "^3.2.0",
    "jsdom": "^16.4.0",
    "ldapjs": "^2.2.0",
    "ldapjs-promise": "^1.0.3",
    "loopback-connector-mongodb": "^5.4.0",
    "multer": "^1.4.2",
    "passport-openidconnect": "^0.1.1",
    "pm2": "^4.5.4",
    "socket.io": "^2.3.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.9",
    "@loopback/eslint-config": "^10.0.5",
    "@loopback/testlab": "^3.2.12",
    "@types/express-session": "^1.17.4",
    "@types/formidable": "^2.0.5",
    "@types/ldapjs": "^1.0.9",
    "@types/node": "^10.17.51",
    "@types/passport": "^1.0.7",
    "eslint": "^7.18.0",
    "source-map-support": "^0.5.19",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.1.3"
  }
}
